{
  "name": "sc-admin-ui-express",
  "version": "0.0.0",
  "private": true,
  "config": {
    "if-present": false
  },
  "scripts": {
    "prestart": "",
    "start": "node ./bin/www",
    "start_test": "NODE_ENV=test node ./bin/www",
    "swdev": "node pre-dev&&set NODE_ENV=development&&nodemon",
    "swprod": "set NODE_ENV=production&&node ./bin/www",
    "start_dev": "node pre-dev && NODE_ENV=development && nodemon",
    "build": "r.js -o ./build/build.js",
    "build_windows": "r.js.cmd -o ./build/build.js",
    "mocha_run_test": "mocha --grep $TEST_NAME",
    "start_expose_gc": "node --expose-gc ./bin/www",
    "eslint_nodejs": "eslint --config ./node.eslintrc --ignore-path ./node.eslintignore .",
    "eslint_browser": "eslint --config ./browser.eslintrc --ignore-path ./browser.eslintignore ./public",
    "gzip": "gzip -9 ./public/static/app/optimized/optimized.js"
  },
  "dependencies": {
    "async": "^1.4.0",
    "bcrypt": "^0.8.4",
    "body-parser": "^1.13.2",
    "cluster": "^0.7.7",
    "colors": "^1.1.0",
    "connect": "^3.3.5",
    "connect-mongo": "^0.8.1",
    "connect-redis": "^2.4.1",
    "cookie": "^0.1.3",
    "cookie-parser": "^1.3.5",
    "ejs": "^2.3.1",
    "events": "^1.0.2",
    "express": "^4.13.1",
    "express-session": "^1.11.3",
    "gulp": "^3.9.0",
    "gulp-param": "^0.6.3",
    "gulp-requirejs-metagen": "^1.0.8",
    "idempotent-json": "^0.8.9",
    "mongoose": "^4.1.0",
    "mongoose-validate": "0.0.5",
    "morgan": "^1.6.1",
    "object-sizeof": "^1.0.6",
    "passport": "^0.2.2",
    "passport-custom": "^1.0.1",
    "passport-local": "^1.0.0",
    "redis": "^0.12.1",
    "replacestream": "^4.0.0",
    "request": "^2.60.0",
    "requirejs-metagen": "^1.1.4",
    "serve-favicon": "^2.3.0",
    "socket.io": "^1.3.5",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "debug": "^2.2.0",
    "gulp": "^3.9.0",
    "gulp-nodemon": "^2.0.4",
    "gulp-react": "^3.0.1",
    "mocha": "^2.2.5"
  }
}
